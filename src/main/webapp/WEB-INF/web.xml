<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Contador</servlet-name>
        <servlet-class>exemploServlet.Contador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImprimeRequestServlet</servlet-name>
        <servlet-class>exemploServlet.ImprimeRequestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TextServlet</servlet-name>
        <servlet-class>exemploServlet.TextServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarServlet</servlet-name>
        <servlet-class>exemploServlet.ValidarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>exemploServlet.ListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Contador</servlet-name>
        <url-pattern>/contador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImprimeRequestServlet</servlet-name>
        <url-pattern>/imprime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TextServlet</servlet-name>
        <url-pattern>/textServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarServlet</servlet-name>
        <url-pattern>/validator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
